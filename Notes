############################################################

- Base de Datos  queries

SELECT * FROM (table);

Es lo mismo:

select * from sakila.actor
where (last_name = 'WILLIS' or last_name='WILLIAMS')
and actor_id >= 83 order by actor_id DESC;


select * from sakila.actor
where last_name IN ('WILLIS','WILLIAMS')
and actor_id >= 83 order by actor_id DESC;


LIKE

select * from sakila.actor
where last_name like '%WILL%'
and actor_id >= 83 order by actor_id DESC;

JOIN LEFT

select a.address_id, a.address, a.district, a.city_id, c.city from address a
left join city c on c.city_id = a.city_id;


DISTINCT

select count(distinct city_id) from address;


select count(*) , a.actor_id, a.first_name, a.last_name, co.title, co.description from actor a
left join film_actor c on c.actor_id = a.actor_id
left join film co on co.film_id=c.actor_id
group by a.actor_id
Order by 1 desc;


SUMA, PROMEDIO, MINIMO, MAXIMO

#Para el promedio
select max(amount) as Maximun, min(amount), avg(amount) from sakila.payment;

JUGAR CON FECHAS

select count(*), year(rental_date), month(rental_date) from sakila.rental
group by 2,3;
